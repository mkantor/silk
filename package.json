{
  "name": "silk",
  "version": "0.0.0",
  "dependencies": {
    "@michijs/htmltype": "^0.4.2",
    "html-entities": "^2.6.0",
    "html-tags": "^5.1.0",
    "web-streams-polyfill": "^4.2.0"
  },
  "devDependencies": {
    "@types/node": "^24.5.1",
    "typescript": "^5.9.2"
  },
  "files": [
    "dist/*"
  ],
  "main": "dist/index.js",
  "repository": "github:mkantor/silk",
